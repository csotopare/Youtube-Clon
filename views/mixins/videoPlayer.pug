mixin videoPlayer(video ={})
   .videoPlayer
      video(src=`/${video.src}`)
      .videoPlayer__controls
         videoPlayer__Column
            span
               i.fas.fa-volume-up
            span
               |00:00 / 10:00
         videoPlayer__Column
            span
               i.fas.fa-play
         videoPlayer__Column
            span
               i.fas.fa-expand
