{
   "name": "wetube",
   "version": "1.0.0",
   "description": "Cloning Youtube with JavaScript and NodeJs",
   "main": "index.js",
   "author": "Carlos Soto",
   "license": "ISC",
   "dependencies": {
      "@babel/cli": "^7.12.10",
      "@babel/core": "^7.12.10",
      "@babel/node": "^7.12.10",
      "@babel/polyfill": "^7.12.1",
      "@babel/preset-env": "^7.12.11",
      "autoprefixer": "^10.2.1",
      "aws-sdk": "^2.824.0",
      "axios": "^0.21.1",
      "babel-loader": "^8.2.2",
      "body-parser": "^1.19.0",
      "connect-mongo": "^3.2.0",
      "cookie-parser": "^1.4.5",
      "css-loader": "^5.0.1",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "express-session": "^1.17.1",
      "extract-text-webpack-plugin": "^3.0.2",
      "get-blob-duration": "^1.1.2",
      "gulp-nodemon": "^2.5.0",
      "helmet": "^4.2.0",
      "jsdom": "^16.4.0",
      "mini-css-extract-plugin": "^1.3.3",
      "mongoose": "^5.11.8",
      "morgan": "^1.10.0",
      "multer": "^1.4.2",
      "multer-s3": "^2.9.0",
      "node-sass": "^5.0.0",
      "passport": "^0.4.1",
      "passport-facebook": "^3.0.0",
      "passport-github": "^1.1.0",
      "passport-local": "^1.0.0",
      "passport-local-mongoose": "^6.0.1",
      "postcss-loader": "^4.1.0",
      "pug": "^3.0.0",
      "sass-loader": "^10.1.0",
      "webpack": "^5.11.0",
      "webpack-cli": "^4.3.0"
   },
   "scripts": {
      "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
      "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
      "build:assets": "cd src && WEBPACK_ENV=production webpack",
      "copyAll": "cp -R src/static build && cp -R src/views build",
      "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
      "build": "npm run build:server && npm run build:assets && npm run copyAll",
      "prebuild": "rm -rf build",
      "tunnel": "ngrok http 4000",
      "start": "node build/init.js",
      "prestart": "npm run build"
   },
   "devDependencies": {
      "eslint": "^7.16.0",
      "nodemon": "^2.0.6"
   }
}
